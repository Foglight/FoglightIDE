    <script-function id="2" display-name="Get Icon Reference" ts="1278064546571" public="false" output-type="wcf:IconReference">
        <uuid>478005ba-602d-46b2-af9c-6658037f9f85</uuid>
        <inputs>
            <input name="icon" type-name="metadata:Icon"/>
        </inputs>
        <script xml:space="preserve"><![CDATA[import com.quest.wcf.core.types.IconReference;
try
{

	return new IconReference (icon.get("fqId",null));
}
catch (Exception e)
{
	System.out.println(e);
}
return null;]]></script>
    </script-function>
    <script-function id="12" display-name="getModulePath" ts="1255081332082" public="false" output-type="wcf:String">
        <uuid>c3f50d93-e89f-401a-9a42-72dd5a8f5259</uuid>
        <inputs>
            <input name="m" type-name="metadata:Module"/>
        </inputs>
        <script xml:space="preserve"><![CDATA[def path = m.name
while (m.	parentModule != null){
m = m.parentModule
path = m.name+"/"+path
}

return path]]></script>
    </script-function>
